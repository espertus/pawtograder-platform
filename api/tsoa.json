{
  "entryFile": "src/index.ts",
  "noImplicitAdditionalProperties": "throw-on-extras",
  "controllerPathGlobs": ["src/api/**/*Controller.ts"],
  "spec": {
    "outputDirectory": "./",
    "specVersion": 3,
    "basePath": "/",
    "securityDefinitions": {
      "supabase": {
        "type": "accessToken",
        "name": "Authorization",
        "in": "header"
      }
    }
  },
  "routes": {
    "authenticationModule": "src/api/authentication.ts",
    "routesDir": "generated",
    "basePath": "/",
    "esm": true
  }
}
